        <class>br.edu.utfpr.cm.minerador.model.svn.Tag</class>
        <class>br.edu.utfpr.cm.minerador.model.svn.TagRevision</class>
        <exclude-unlisted-classes>true</exclude-unlisted-classes>
        <properties/>
